{
  "name": "diside/security-bundle",
  "autoload": {
    "psr-0": {"Diside\\SecurityBundle": ""}
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/disider/Diside\SecurityComponent"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "diside/security-component": "dev-master"
  },
  "require-dev": {
    "symfony/symfony": "2.5.*",
    "doctrine/orm": "~2.2,>=2.2.3",
    "doctrine/doctrine-bundle": "~1.2",
    "twig/extensions": "~1.0",
    "symfony/assetic-bundle": "~2.3",
    "symfony/swiftmailer-bundle": "~2.3",
    "symfony/monolog-bundle": "~2.4",
    "sensio/distribution-bundle": "~3.0",
    "sensio/framework-extra-bundle": "~3.0",
    "incenteev/composer-parameter-handler": "~2.0",

    "phpunit/phpunit": "4.0.*",
    "mockery/mockery": "~0.9",
    "doctrine/doctrine-fixtures-bundle": "2.2.*"
  },
  "config": {
    "bin-dir": "bin/"
  },
  "target-dir": "Diside/SecurityBundle"
