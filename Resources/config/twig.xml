<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="diside.security.twig.icon_extension.class">Diside\SecurityBundle\Twig\IconExtension</parameter>
    </parameters>

    <services>

        <service id="diside.security.twig.icon_extension" class="%diside.security.twig.icon_extension.class%">
            <tag name="twig.extension"/>
        </service>

    </services>
</container>
