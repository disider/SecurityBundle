<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="diside.interactor.security_interactor_register.class">Diside\SecurityComponent\Interactor\SecurityInteractorRegister</parameter>
        <parameter key="diside.interactor.interactor_factory.class">Diside\SecurityComponent\Interactor\InteractorFactory</parameter>
    </parameters>

    <services>

        <service id="diside.interactor.security_interactor_register" class="%diside.interactor.security_interactor_register.class%">
        </service>

        <service id="interactor_factory" class="%diside.interactor.interactor_factory.class%">
            <argument type="service" id="diside.gateway_register" />
            <argument type="service" id="diside.logger" />
            <call method="addRegister">
                <argument type="service" id="diside.interactor.security_interactor_register" />
            </call>
        </service>

    </services>

</container>
